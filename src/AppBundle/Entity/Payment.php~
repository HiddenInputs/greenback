<?php

namespace AppBundle\Entity;

/**
 * Payment
 */
class Payment
{
    /**
     * @var int
     */
    private $id;

    /**
     * @var string
     */
    private $name;

    /**
     * @var \DateTime $createdAt
     */
    private $createdAt;


    public function __construct()
    {
        $this->createdAt = new \DateTime();
    }

}
